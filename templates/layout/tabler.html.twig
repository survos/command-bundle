<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{% block title %}Command Runner{% endblock %}</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="https://cdn.jsdelivr.net/npm/@tabler/core@1.0.0-beta21/dist/css/tabler.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/@tabler/core@1.0.0-beta21/dist/js/tabler.min.js" defer></script>
</head>
<body>

<div class="page">
    <header class="navbar navbar-expand-md navbar-light">
        <div class="container-xl">
            <a class="navbar-brand" href="{{ path('survos_commands') }}">
                Symfony Commands
            </a>
        </div>
    </header>

    <div class="page-wrapper">
        <div class="container-xl mt-3">
            {% block body %}{% endblock %}
        </div>
    </div>
</div>

</body>
</html>
